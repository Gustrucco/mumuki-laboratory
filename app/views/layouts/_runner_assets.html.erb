<% (language.send "#{prefix}_js_urls").each do |js| %>
  <script src="<%=js%>"></script>
<% end %>

<% (language.send "#{prefix}_css_urls").each do |css| %>
  <link rel="stylesheet" href="<%=css%>" />
<% end %>

<% (language.send "#{prefix}_html_urls").each do |html| %>
  <link rel="import" href="<%=html%>" />
<% end %>
