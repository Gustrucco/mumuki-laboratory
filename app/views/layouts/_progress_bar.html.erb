<div class="progress-list-flex">
  <% guide.exercises.each do |e| %>
    <a data-turbolinks="<%= !e.input_kids? %>" href="<%= exercise_path(e)%>" aria-label="<%= e.navigable_name %>" title="<%= e.navigable_name %>"  class="<%= class_for_progress_list_item(e, e == actual)%>">
    </a>
  <% end %>
</div>
